{
  "name": "wtf-code",
  "private": true,
  "version": "1.0.0",
  "engines": {
    "node": "16.x",
    "npm": "8.x"
  },
"scripts": {
    "dev": "env-cmd -e developement vite",
    "build": "env-cmd -e production vite build",
    "preview": "vite preview",
    "start:dev": "concurrently \"npm run dev\" \"cd server && npm run start\" ",
    "start": "env-cmd -e production node server/server.js",
    "server": "cd server server.js --ignore client",
    "heroku-install": "cd server && npm i && cd ../client && npm i",
    "client": "cd client && npm start",
    "postinstall": "vite build"
  },
  "dependencies": {
    "@apollo/client": "^3.7.1",
    "@codemirror/lang-javascript": "^6.1.2",
    "@mailchimp/mailchimp_transactional": "^1.0.50",
    "@uiw/codemirror-themes": "^4.19.1",
    "@uiw/react-codemirror": "^4.19.1",
    "antd": "^5.0.5",
    "apollo-server-express": "^3.11.1",
    "bcrypt": "^5.1.0",
    "concurrently": "^7.6.0",
    "dotenv": "^16.0.3",
    "env-cmd": "^10.1.0",
    "express": "^4.18.2",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^3.1.2",
    "mongodb": "^4.12.1",
    "mongoose": "^6.7.5",
    "openai": "^3.1.0",
    "path": "^0.12.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.4.4",
    "react-router-dom": "^6.4.4",
    "@vitejs/plugin-react": "^2.2.0",
    "vite": "^3.2.5"
  },
  "devDependencies": {
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.8"
  }
}
