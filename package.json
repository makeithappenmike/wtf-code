{
  "name": "wtf-code",
  "private": true,
  "version": "1.0.0",
  "proxy": "http://localhost:3001/",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "start:dev": "concurrently \"cd client && npm run build\" \"cd server && npm run server\" ",
